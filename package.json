{
  "private": true,
  "scripts": {
    "start": "web-ext run -s builds/debug",
    "watch": "npm run dev -- --watch",
    "clean": "rimraf builds/",
    "dev": "cross-env BROWSER=firefox WEBPACK_ENV=dev webpack",
    "qa": "cross-env BROWSER=firefox WEBPACK_ENV=qa webpack",
    "beta": "cross-env BROWSER=firefox WEBPACK_ENV=beta webpack",
    "beta:release": "node script/beta-release",
    "public": "cross-env BROWSER=firefox WEBPACK_ENV=public webpack",
    "test": "npm run unit && npm run e2e",
    "unit": "cross-env BROWSER=firefox jest --coverage",
    "e2e": "npm run e2e:build && npm run e2e:run",
    "e2e:build": "cross-env BROWSER=firefox WEBPACK_ENV=e2e webpack",
    "e2e:run": "cross-env BROWSER=firefox BUILD=e2e mocha --opts test/e2e/mocha.opts test/e2e/**/*.spec.ts",
    "e2e:run:custom": "cross-env BROWSER=firefox BUILD=e2e mocha --opts test/e2e/mocha.opts",
    "translation:import": "grunt oneskyImport",
    "translation:export": "grunt oneskyExport",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint \"src/**/*.js\"",
    "lint:test": "tslint \"test/**/**.ts\" -p tsconfig.json"
  },
